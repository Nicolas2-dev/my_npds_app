{
    "name": "nicolas2-dev/my_npds_fram",
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Modules\\": "app/modules/",
            "Themes\\": "app/themes/",
            "Shared\\": "app/shared/",
            "Npds\\": "npds/"
        },
        "files": [
            "app/Support/Helpers.php",
            "npds/Support/Helpers.php"
        ] 
    },   
    "authors": [
        {
            "name": "nicolas2",
            "email": "nicolas.l.devoy@free.fr"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "psr/container": "^1.0",
        "phpmailer/phpmailer": "dev-master",
        "php81_bc/strftime": "dev-master",
        "doctrine/inflector": "1.0.*",
        "phpfastcache/phpfastcache": "3.0.*"

    },
    "require-dev": {
        "symfony/var-dumper": "7.2.x-dev"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
